---
---
{
  "type": "FeatureCollection",
  "features": [{% for estate in site.estates %}
    {
      "type": "Feature",
      "properties": {
        "title": "{{ estate.name }}",
        "image": "{{ estate.image_path }}",
        "url": "{{estate.url}}"
      },
      "geometry": {
	"type": "Point",
	"coordinates": ["{{ estate.coordinates }}"]
        }
      }{% if forloop.rindex > 1 %},{% endif %}{% endfor %}
  ]
}

